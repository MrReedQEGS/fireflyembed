</head>
<body>
  <h1>Python in the Browser example</h1>
  <pre id="output">Loading...</pre>
  
  <script>
  console.log("Hello");
  </script>

  <script>
  function myFunction() {
    alert("I am an alert box!");
  }
  </script>

  <script>
    async function main() {
      const pyodide = await loadPyodide();
      const result = pyodide.runPython(`
print("Hello, world - Mr Reed!")
      `);
      document.getElementById("output").textContent =
        pyodide.runPython("import sys; sys.stdout.getvalue()");
    }
    main();
  </script>
